var Behavior,
  extend = function(child, parent) { for (var key in parent) { if (hasProp.call(parent, key)) child[key] = parent[key]; } function ctor() { this.constructor = child; } ctor.prototype = parent.prototype; child.prototype = new ctor(); child.__super__ = parent.prototype; return child; },
  hasProp = {}.hasOwnProperty;

Behavior = (function() {
  Behavior.StepNamingConventionViolation = (function(superClass) {
    extend(StepNamingConventionViolation, superClass);

    function StepNamingConventionViolation(stepName) {
      StepNamingConventionViolation.__super__.constructor.call(this, "Step with name `" + stepName + "` already exists. Step names must be unique");
    }

    return StepNamingConventionViolation;

  })(Error);

  function Behavior(api, embedder) {
    this.api = api;
    this.embedder = embedder;
    this._steps = null;
    this._flow = null;
    this._queue = null;
    this._finishDef = null;
  }

  Behavior.prototype.step = function(name, func, maxAttempts) {
    var err;
    if (maxAttempts == null) {
      maxAttempts = 1;
    }
    if (this._steps[name] != null) {
      err = new Behavior.StepNamingConventionViolation(name);
      this._finishDef.reject(err);
      throw err;
    }
    this._steps[name] = {
      func: func,
      maxAttempts: maxAttempts
    };
    if (this._flow.length > 0) {
      this._steps[this._flow.slice(0).pop()].next = name;
    }
    return this._flow.push(name);
  };

  Behavior.check = function(method) {
    return function(ifSuccess, ifFail) {
      return (function(_this) {
        return function() {
          var def, resolve;
          def = RSVP.defer();
          resolve = function(success) {
            if (success == null) {
              success = true;
            }
            setTimeout(function() {
              return _this._performStep(success ? ifSuccess : ifFail);
            });
            return def.reject();
          };
          method.call(_this, resolve, def.reject);
          return def.promise;
        };
      })(this);
    };
  };

  Behavior.action = function(method) {
    return function() {
      var def;
      def = RSVP.defer();
      method.call(this, def.resolve, def.reject);
      if (debugMode) {
        try {
          $.log(this.api.inspect(), "info");
        } catch (_error) {}
        try {
          $.log(this.embedder.inspect(), "info");
        } catch (_error) {}
      }
      return def.promise;
    };
  };

  Behavior.prototype.perform = function() {
    this._steps = [];
    this._flow = [];
    this._queue = [];
    this._finishDef = RSVP.defer();
    this.flow();
    this.step("finish", this.finish);
    this._performStep(this._flow[0]);
    return this._finishDef.promise;
  };

  Behavior.prototype._performStep = function(name, maxAttempts) {
    var actuallyPerform, def, step;
    def = RSVP.defer();
    step = this._steps[name];
    if (maxAttempts == null) {
      maxAttempts = step.maxAttempts;
    }
    actuallyPerform = (function(_this) {
      return function() {
        return step.func.call(_this).then(function() {
          def.resolve;
          if (step.next) {
            return setTimeout(function() {
              return _this._performStep(step.next);
            });
          }
        })["catch"](function() {
          return _this._performStep(name, maxAttempts - 1);
        });
      };
    })(this);
    if (maxAttempts === 0) {
      def.reject();
    } else {
      if (this._queue.length === 0) {
        actuallyPerform();
      } else {
        this._queue.slice(0).pop().then((function(_this) {
          return function() {
            return actuallyPerform();
          };
        })(this));
      }
    }
    this._steps.push(def.promise);
    return def.promise;
  };

  Behavior.prototype.finish = Behavior.action(function(resolve, reject) {
    return this._finishDef.resolve();
  });

  return Behavior;

})();

var extend = function(child, parent) { for (var key in parent) { if (hasProp.call(parent, key)) child[key] = parent[key]; } function ctor() { this.constructor = child; } ctor.prototype = parent.prototype; child.prototype = new ctor(); child.__super__ = parent.prototype; return child; },
  hasProp = {}.hasOwnProperty;

Behavior.Standard = (function(superClass) {
  extend(Standard, superClass);

  function Standard() {
    return Standard.__super__.constructor.apply(this, arguments);
  }

  Standard.prototype.fetchEndpointsFromViroolApi = Standard.action(function(resolve, reject) {
    $.log("fetchEndpointsFromViroolApi", "info");
    return this.api.fetchEndpoints().then(resolve)["catch"](reject);
  });

  Standard.prototype.haveViroolEndpoint = Standard.check(function(resolve) {
    var endpoint, i, len, ref;
    $.log("checkIfHaveViroolEndpoint", "info");
    ref = this.api.getEndpoints();
    for (i = 0, len = ref.length; i < len; i++) {
      endpoint = ref[i];
      if (!(endpoint instanceof ViroolEndpoint)) {
        continue;
      }
      this.api.setEndpointComparator(function(l, r) {
        return l.attrs.priority - r.attrs.priority;
      });
      return resolve(true);
    }
    return resolve(false);
  });

  Standard.prototype.fetchExternalEndpoints = Standard.action(function(resolve, reject) {
    $.log("fetchExternalEndpoints", "info");
    return this.api.fetchExternalEndpoints().then(resolve);
  });

  Standard.prototype.auction = Standard.action(function(resolve, reject) {
    $.log("runAuction", "info");
    this.api.addEndpointFilter(function(endpoint) {
      return endpoint.couldBeShown();
    });
    this.api.setEndpointComparator(function(l, r) {
      if (l.price === r.price) {
        return l.attrs.priority - r.attrs.priority;
      } else {
        return l.price - r.price;
      }
    });
    return resolve();
  });

  Standard.prototype.pickEndpoint = Standard.action(function(resolve, reject) {
    $.log("pickEndpoint", "info");
    this.api.setPickedEndpoint(this.api.getEndpoints()[0]);
    return resolve();
  });

  Standard.prototype.isEndpointPicked = Standard.check(function(resolve) {
    $.log("isEndpointPicked", "info");
    return resolve(this.api.getPickedEndpoint() != null);
  });

  Standard.prototype.findContainer = Standard.action(function(resolve, reject) {
    $.log("findContainer", "info");
    this.embedder.setContainer(document.scripts[document.scripts.length - 1]);
    return resolve();
  });

  Standard.prototype.containerExists = Standard.check(function(resolve) {
    $.log("containerExists", "info");
    return resolve(this.embedder.container != null);
  });

  Standard.prototype.embed = Standard.action(function(resolve, reject) {
    $.log("embed", "info");
    return this.embedder.inject(this.api.getPickedEndpoint()).then(resolve)["catch"](reject);
  });

  Standard.prototype.isEmbedded = Standard.check(function(resolve) {
    $.log("checkIfEmbedded", "info");
    return resolve(true);
  });

  Standard.prototype.flow = function() {
    this.step("fetchEndpoints", this.fetchEndpointsFromViroolApi, 3);
    this.step("checkIfHaveViroolEndpoint", this.haveViroolEndpoint("pickEndpoint", "fetchExternalEndpoints"));
    this.step("fetchExternalEndpoints", this.fetchExternalEndpoints);
    this.step("runAuction", this.auction);
    this.step("pickEndpoint", this.pickEndpoint);
    this.step("checkIfEndpointPicked", this.isEndpointPicked("findContainer", "finish"));
    this.step("findContainer", this.findContainer);
    this.step("checkIfContainerExists", this.containerExists("embed", "finish"));
    this.step("embed", this.embed);
    return this.step("checkIfEmbedded", this.isEmbedded("finish", "pickEndpoint"));
  };

  return Standard;

})(Behavior);

var extend = function(child, parent) { for (var key in parent) { if (hasProp.call(parent, key)) child[key] = parent[key]; } function ctor() { this.constructor = child; } ctor.prototype = parent.prototype; child.prototype = new ctor(); child.__super__ = parent.prototype; return child; },
  hasProp = {}.hasOwnProperty;

Behavior.IgnoreViroolEndpoints = (function(superClass) {
  extend(IgnoreViroolEndpoints, superClass);

  function IgnoreViroolEndpoints() {
    return IgnoreViroolEndpoints.__super__.constructor.apply(this, arguments);
  }

  IgnoreViroolEndpoints.prototype.haveViroolEndpoint = IgnoreViroolEndpoints.check(function(resolve) {
    $.log("checkIfHaveViroolEndpoint", "info");
    this.api.addEndpointFilter(function(endpoint) {
      return endpoint instanceof ExternalEndpoint;
    });
    return resolve(false);
  });

  return IgnoreViroolEndpoints;

})(Behavior.Standard);

var Embedder;

Embedder = (function() {
  var log;

  log = Helpers.log;

  function Embedder() {
    var BehaviorClass;
    this.container = null;
    this.api = new ViroolApi();
    BehaviorClass = this.api.getBehaviorClass();
    this.behavior = new BehaviorClass(this.api, this);
  }

  Embedder.prototype.embed = function() {
    return this.behavior.perform().then((function(_this) {
      return function() {
        _this.api.sendFillRateNotification();
        return $.log("The embed process has successfully finished", "info");
      };
    })(this));
  };

  Embedder.prototype.inject = function(endpoint) {
    var def;
    def = RSVP.defer();
    new Renderer(this.container, endpoint.getEmbedParams()).render();
    def.resolve();
    return def.promise;
  };

  Embedder.prototype.setContainer = function(container) {
    this.container = container;
  };

  Embedder.prototype.inspect = function() {
    return {
      container: this.container
    };
  };

  return Embedder;

})();

RSVP.on("error", function(reason) {
  return $.log(reason, "error");
});

Flash.ready.then(function() {
  var embedder;
  embedder = new Embedder();
  return embedder.embed();
});

}).call(window.___virool___ = {});