// (c) 2015 Virool Inc.
(function() {
  var Renderer;

Renderer = (function() {
  var vastSrcBase;

  vastSrcBase = function() {
    return "https://api.virool.com/widgets/" + siteKey + "/169398?" + qs;
  };

  function Renderer(el, config) {
    this.el = el;
    this.config = config != null ? config : {};
  }

  Renderer.prototype.withSize = function(src) {
    var dimension, i, len, ref, regexp;
    ref = ["width", "height"];
    for (i = 0, len = ref.length; i < len; i++) {
      dimension = ref[i];
      regexp = RegExp(dimension + "=[\\d.]*");
      src = regexp.test(src) ? src.replace(regexp, dimension + "=" + this.config[dimension]) : src + "&" + dimension + "=" + this.config[dimension];
    }
    return src + "&backfill=1";
  };

  Renderer.prototype.formatString = function() {
    switch (this.config.placement) {
      case "inline":
        return "format=intext";
      case "js":
        return "format=js";
      default:
        return "";
    }
  };

  Renderer.prototype.vastSrc = function() {
    var priceParam;
    priceParam = this.config.price != null ? "&price=" + this.config.price : "";
    return "https://api.virool.com/widgets/" + this.config.siteKey + "/" + this.config.cid + "?" + (this.formatString()) + "&vast_xml_enabled=1" + priceParam + "&" + ($.URI.toQS(this.config.legacyParams));
  };

  Renderer.prototype.unitSrc = function() {
    var baseSrc, params, src;
    src = this.config.type === "vast" ? this.vastSrc() : (baseSrc = this.config.src.split("?")[0], params = $.extend($.URI.parseQs(this.config.src.split("?")[1]), this.config.legacyParams), this.config.src + "?" + ($.URI.toQS(params)));
    return this.withSize(src);
  };

  Renderer.prototype.onPlayerReady = function() {
    return Helpers.DOM.PostMessage.addJsonListener((function(_this) {
      return function(data, event) {
        var msg;
        if (data && data.event === 'vast_player_ready') {
          msg = JSON.stringify({
            event: 'vast_xml',
            value: _this.config.data
          });
          return Helpers.DOM.PostMessage.sendTo(event.source, msg);
        }
      };
    })(this));
  };

  Renderer.prototype.renderJS = function() {
    var script;
    script = document.createElement("script");
    script.src = this.unitSrc();
    script.type = "text/javascript";
    return this.el.parentNode.insertBefore(script, this.el);
  };

  Renderer.prototype.renderIframe = function(params) {
    var target;
    target = document.createElement("div");
    this.el.appendChild(target);
    return target.outerHTML = "<iframe\n  src=\"" + (this.unitSrc()) + "\"\n  width=\"" + this.config.width + "\"\n  height=\"" + this.config.height + "\"\n  frameborder=\"0\"\n  marginheight=\"0\"\n  marginwidth=\"0\"\n  scrolling=\"no\"></iframe>";
  };

  Renderer.prototype.render = function() {
    switch (this.config.type) {
      case 'html':
        this.renderIframe();
        break;
      default:
        this.renderJS();
    }
    return this.onPlayerReady();
  };

  return Renderer;

})();

