}).call(this);


var Flash;

Flash = (function() {
  var SWF_PATH, _readyDeferred, callbacks, lastCbId;

  SWF_PATH = false ? "/AdManager.swf" : "//djkjukpjwja60.cloudfront.net/admanager/AdManager.swf";

  Flash.isEmbedded = false;

  lastCbId = 0;

  callbacks = {};

  _readyDeferred = RSVP.defer();

  Flash.ready = _readyDeferred.promise;

  Flash.params = {
    bgcolor: "#FFFFFF",
    allowScriptAccess: "always"
  };

  Flash.attributes = {
    name: "default",
    styleclass: "___virool___",
    align: "right"
  };

  Flash.containerId = "___viroolFlash" + (+(new Date)) + "___";

  Flash.prototype._onSuccess = function(callbackId, data) {
    return setTimeout(function() {
      return callbacks[callbackId].resolve(data);
    });
  };

  Flash.prototype._onFail = function(callbackId, status) {
    return setTimeout(function() {
      return callbacks[callbackId].reject(status);
    });
  };

  Flash.prototype._onReady = _readyDeferred.resolve;

  Flash.node = function() {
    return document.getElementById(Flash.containerId);
  };

  function Flash() {
    var container, flashvars;
    container = document.createElement("div");
    container.id = Flash.containerId;
    document.body.appendChild(container);
    swfobject.embedSWF(SWF_PATH, Flash.containerId, "1", "1", "9.0.0", false, flashvars = {}, Flash.params, Flash.attributes);
  }

  Flash.get = function(url) {
    var cbName, deferred;
    lastCbId++;
    cbName = "cb" + lastCbId;
    deferred = callbacks[cbName] = RSVP.defer();
    setTimeout(function() {
      return Flash.node().get(cbName, url);
    });
    return deferred.promise;
  };

  return Flash;

})();

if (!Flash.isEmbedded) {
  this._flash = new Flash();
}

var $, Helpers, debugMode, devEnv, pauseOnErrors,
  slice = [].slice;

debugMode = /virool_debug=1/.test(location.href);

pauseOnErrors = /virool_pause_on_errors=1/.test(location.href);

devEnv = false;

$ = Helpers = {
  log: function(msg, type) {
    if (type == null) {
      type = "log";
    }
    if (pauseOnErrors && type === "error") {
      debugger;
    }
    if (debugMode && (msg != null)) {
      return typeof console !== "undefined" && console !== null ? typeof console[type] === "function" ? console[type]("[Virool: " + (new Date()) + "]:", msg) : void 0 : void 0;
    }
  },
  extend: function() {
    var extended, i, idx, key, len, object, objects, val;
    objects = 1 <= arguments.length ? slice.call(arguments, 0) : [];
    extended = objects[0];
    for (idx = i = 0, len = objects.length; i < len; idx = ++i) {
      object = objects[idx];
      if (idx !== 0) {
        for (key in object) {
          val = object[key];
          if (Object.prototype.hasOwnProperty.call(object, key)) {
            extended[key] = val;
          }
        }
      }
    }
    return extended;
  },
  DOM: {
    parseXML: function(txt) {
      var parser, xmlDoc;
      if (window.DOMParser) {
        parser = new DOMParser();
        xmlDoc = parser.parseFromString(txt, "text/xml");
      } else {
        xmlDoc = new ActiveXObject("Microsoft.XMLDOM");
        xmlDoc.async = false;
        xmlDoc.loadXML(txt);
      }
      return xmlDoc;
    },
    getElementAttr: function(ele, attr) {
      var a, i, len, ref, result;
      result = typeof ele.getAttribute === "function" ? ele.getAttribute(attr) : void 0;
      if (!result) {
        ref = ele.attributes;
        for (i = 0, len = ref.length; i < len; i++) {
          a = ref[i];
          if (a.nodeName === attr) {
            return attr;
          }
        }
      }
      return result;
    },
    PostMessage: (function(window) {
      var eventMethod, eventer, jsonMessageHandler, jsonSubscribers, messageEvent, messageHandler, subscribers;
      eventMethod = (window.addEventListener ? "addEventListener" : "attachEvent");
      eventer = window[eventMethod];
      messageEvent = (eventMethod === "attachEvent" ? "onmessage" : "message");
      subscribers = [];
      jsonSubscribers = [];
      jsonMessageHandler = function(event) {
        var e, i, len, parsedMessage, results, subscriber;
        try {
          parsedMessage = JSON.parse(event.data);
          results = [];
          for (i = 0, len = jsonSubscribers.length; i < len; i++) {
            subscriber = jsonSubscribers[i];
            results.push(subscriber.call(this, parsedMessage, event));
          }
          return results;
        } catch (_error) {
          e = _error;
        }
      };
      messageHandler = function(event) {
        var i, len, subscriber;
        for (i = 0, len = subscribers.length; i < len; i++) {
          subscriber = subscribers[i];
          subscriber.call(this, event);
        }
        if (jsonSubscribers.length) {
          jsonMessageHandler(event);
        }
        return void 0;
      };
      eventer(messageEvent, messageHandler, false);
      return {
        addListener: function(func) {
          if (typeof func === 'function') {
            return subscribers.push(func);
          }
        },
        addJsonListener: function(func) {
          if (typeof func === 'function') {
            return jsonSubscribers.push(func);
          }
        },
        sendTo: function(windowObj, message, origin) {
          if (origin == null) {
            origin = '*';
          }
          return windowObj.postMessage(message, origin);
        },
        sendToParent: function(message, origin) {
          if (origin == null) {
            origin = '*';
          }
          return this.sendTo(window.parent, message, origin);
        },
        sendToFrame: function(iframe, message, origin) {
          if (origin == null) {
            origin = '*';
          }
          return this.sendTo(iframe.contentWindow, message, origin);
        }
      };
    })(window)
  },
  URI: {
    parseQs: function(qs) {
      var attr, fn, i, len, parsed, ref;
      parsed = {};
      ref = qs.replace(/^\?/, "").split("&");
      fn = function() {
        var split;
        split = attr.split("=");
        return parsed[decodeURIComponent(split[0])] = split[1] != null ? decodeURIComponent(split[1]) : true;
      };
      for (i = 0, len = ref.length; i < len; i++) {
        attr = ref[i];
        fn();
      }
      return parsed;
    },
    toQS: function(obj, prefix) {
      var k, res, v;
      if (obj == null) {
        obj = {};
      }
      res = [];
      for (k in obj) {
        v = obj[k];
        k = encodeURIComponent(k);
        if (Array.isArray(obj)) {
          k = "";
        }
        switch (typeof v) {
          case "object":
            if (prefix != null) {
              res.push($.URI.toQS(v, prefix + "[" + k + "]"));
            } else {
              res.push($.URI.toQS(v, k));
            }
            break;
          case "boolean":
            res.push(prefix ? prefix + "[" + k + "]" : k);
            break;
          default:
            res.push((prefix ? prefix + "[" + k + "]" : k) + "=" + (encodeURIComponent(v)));
        }
      }
      return res.join("&");
    }
  }
};

var Endpoint;

Endpoint = (function() {
  Endpoint.makeEndpoint = function(rawData) {
    if (rawData == null) {
      rawData = {};
    }
    if (rawData.priority === 0) {
      return new ViroolEndpoint(rawData);
    } else {
      return new ExternalEndpoint(rawData);
    }
  };

  function Endpoint(attrs) {
    this.attrs = attrs != null ? attrs : {};
  }

  Endpoint.prototype.couldBeShown = function() {
    return true;
  };

  Endpoint.prototype.getEmbedParams = function() {
    return {
      width: ViroolApi.config.width,
      height: ViroolApi.config.height,
      placement: "inline",
      type: this.attrs.type,
      src: this.attrs.url,
      cid: this.attrs.cid,
      container: "__virool__",
      siteKey: ViroolApi.config.siteKey,
      legacyParams: Helpers.URI.parseQs(ViroolApi.params)
    };
  };

  return Endpoint;

})();

