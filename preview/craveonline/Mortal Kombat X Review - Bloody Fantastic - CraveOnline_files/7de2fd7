ExternalEndpoint = (function(superClass) {
  extend(ExternalEndpoint, superClass);

  function ExternalEndpoint() {
    return ExternalEndpoint.__super__.constructor.apply(this, arguments);
  }

  ExternalEndpoint.prototype._calculatePrice = function() {
    var ref, ref1, ref2, ref3, ref4, vastPrice;
    vastPrice = (ref = this.vast.getElementsByTagName("Price")) != null ? (ref1 = ref[0]) != null ? (ref2 = ref1.innerHTML) != null ? (ref3 = ref2.match(/[\d\.]+/)) != null ? ref3[0] : void 0 : void 0 : void 0 : void 0;
    if (vastPrice != null) {
      return parseFloat(vastPrice) * 100;
    } else {
      return (ref4 = this.attrs.price) != null ? ref4 : 0;
    }
  };

  ExternalEndpoint.prototype.fetch = function() {
    var def;
    return def = Flash.get(this.attrs.url).then((function(_this) {
      return function(vast) {
        _this.rawVast = vast;
        _this.vast = Helpers.DOM.parseXML(vast);
        return _this.price = _this._calculatePrice();
      };
    })(this));
  };

  ExternalEndpoint.prototype.couldBeShown = function() {
    if (this.vast == null) {
      return false;
    }
    return this.vast.getElementsByTagName("Ad").length > 0;
  };

  ExternalEndpoint.prototype.getEmbedParams = function() {
    var params;
    params = ExternalEndpoint.__super__.getEmbedParams.apply(this, arguments);
    params.data = this.getEncodedVast();
    params.price = ~~this.price;
    return params;
  };

  ExternalEndpoint.prototype.getEncodedVast = function() {
    return Base64.encode(this.rawVast);
  };

  return ExternalEndpoint;

})(Endpoint);

var ViroolApi;

ViroolApi = (function() {
  ViroolApi.params = 'format=inline&backfill=1&nologo=1&height=393.75&ap=1&or=http://www.craveonline.com/gaming/articles/848025-mortal-kombat-x-review-bloody-fantastic&no_close_button=1&in_iframe=1&width=630&skip_iframe_check=1';

  ViroolApi.config = JSON.parse('{"width":"630","height":"393.75","siteKey":"7de2fd7","reverse":false}');

  function ViroolApi() {
    this.endpoints = null;
    this._endpointFilters = [];
    this._endpointComparator = null;
    this._pickedEndpoint = null;
  }

  ViroolApi.prototype.fetchEndpoints = function() {
    var def, fetch;
    def = RSVP.defer();
    fetch = Flash.get(devEnv ? "http://localhost:8000/endpoints.json" : "https://api.virool.com/admanager/" + ViroolApi.config.siteKey + "?" + ViroolApi.params);
    fetch.then((function(_this) {
      return function(data) {
        var rawEndpoint;
        _this.endpoints = (function() {
          var i, len, ref, results;
          ref = JSON.parse(data);
          results = [];
          for (i = 0, len = ref.length; i < len; i++) {
            rawEndpoint = ref[i];
            results.push(Endpoint.makeEndpoint(rawEndpoint));
          }
          return results;
        })();
        return def.resolve();
      };
    })(this));
    fetch["catch"]((function(_this) {
      return function(status) {
        $.log("failed to fetch endpoints", "error");
        return def.reject();
      };
    })(this));
    return def.promise;
  };

  ViroolApi.prototype.fetchExternalEndpoints = function() {
    var endpoint;
    return RSVP.allSettled((function() {
      var i, len, ref, results;
      ref = this.endpoints;
      results = [];
      for (i = 0, len = ref.length; i < len; i++) {
        endpoint = ref[i];
        if (endpoint instanceof ExternalEndpoint) {
          results.push(endpoint.fetch());
        }
      }
      return results;
    }).call(this));
  };

  ViroolApi.prototype.getBehaviorClass = function() {
    var ref;
    return Behavior[(ref = ViroolApi.config.behavior) != null ? ref : "Standard"];
  };

  ViroolApi.prototype.addEndpointFilter = function(fn) {
    return this._endpointFilters.push(fn);
  };

  ViroolApi.prototype.setEndpointComparator = function(_endpointComparator) {
    this._endpointComparator = _endpointComparator;
  };

  ViroolApi.prototype.getEndpoints = function() {
    var endpoint, endpoints, fn1, i, len, ref;
    endpoints = [];
    ref = this.endpoints;
    fn1 = (function(_this) {
      return function() {
        var filter, j, len1, ref1;
        ref1 = _this._endpointFilters;
        for (j = 0, len1 = ref1.length; j < len1; j++) {
          filter = ref1[j];
          if (!filter.call(_this, endpoint)) {
            return;
          }
        }
        return endpoints.push(endpoint);
      };
    })(this);
    for (i = 0, len = ref.length; i < len; i++) {
      endpoint = ref[i];
      fn1();
    }
    if (typeof this._endpointComparator === "function") {
      endpoints.sort(this._endpointComparator);
    }
    return endpoints;
  };

  ViroolApi.prototype.setPickedEndpoint = function(_pickedEndpoint) {
    this._pickedEndpoint = _pickedEndpoint;
  };

  ViroolApi.prototype.getPickedEndpoint = function() {
    return this._pickedEndpoint;
  };

  ViroolApi.prototype.sendFillRateNotification = function() {
    var def, endpoint, endpoints, i, img, len, message, ref;
    def = RSVP.defer();
    message = {
      e: endpoints = {}
    };
    ref = this.endpoints;
    for (i = 0, len = ref.length; i < len; i++) {
      endpoint = ref[i];
      if (endpoint instanceof ExternalEndpoint) {
        endpoints[endpoint.attrs.cid] = endpoint.couldBeShown() ? 1 : 0;
      }
    }
    img = document.createElement("img");
    img.src = "//track.virool.com/admanager/" + ViroolApi.config.siteKey + "?" + ($.URI.toQS(message));
    img.width = img.height = 1;
    img.onload = def.resolve;
    img.onerror = def.reject;
    document.body.appendChild(img);
